{
  "name": "frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "prettier": "prettier '*.js'",
    "test": "jest"
  },
  "dependencies": {
    "react": "16.6.0-alpha.8af6728",
    "react-native": "0.57.4"
  },
  "devDependencies": {
    "babel-jest": "23.6.0",
    "husky": "^1.1.3",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.49.0",
    "prettier": "1.15.1",
    "pretty-quick": "^1.8.0",
    "react-test-renderer": "16.6.0-alpha.8af6728"
  },
  "jest": {
    "preset": "react-native"
  },
  "prettier": {
    "jsxBracketSameLine": false,
    "semi": false,
    "singleQuote": true,
    "insertPragma": true,
    "trailingComma": "all",
    "useTabs": false,
    "jsxSingleQuote": false,
    "tabWidth": 2,
    "bracketSpacing": false,
    "printWidth": 80
